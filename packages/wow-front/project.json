{
    "name": "wow-front",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/wow-front/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/vite:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/packages/wow-front"
            },
            "configurations": {
                "development": {},
                "production": {}
            }
        },
        "serve": {
            "executor": "@nrwl/vite:dev-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "wow-front:build",
                "proxyConfig": "packages/wow-front/proxy.conf.json"
            },
            "configurations": {
                "development": {
                    "buildTarget": "wow-front:build:development",
                    "hmr": true
                },
                "production": {
                    "buildTarget": "wow-front:build:production",
                    "hmr": false
                }
            }
        },
        "test": {
            "executor": "@nrwl/vite:test",
            "outputs": ["{projectRoot}/coverage"],
            "options": {
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/wow-front/**/*.{ts,tsx,js,jsx}"]
            }
        }
    },
    "tags": []
}
